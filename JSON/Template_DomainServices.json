{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters":
	{
		"Domain Name":
		{
			"type": "string"
		},
		"Virtual Network ID":
		{
			"type": "string"
		},
		"Virtual Network Subnet ID":
		{
			"type": "string"
		}
	},
	"variables":
	{
	},
	"functions":
	[
	],
	"resources":
	[
		{
			"comments": "Domain Services",
			"type": "Microsoft.AAD/domainServices",
			"apiVersion": "2017-01-01",
			"name": "[ parameters( 'Domain Name' ) ]",
			"location": "[ resourceGroup().location ]",
			"properties":
			{
				"domainName": "parameters( 'Domain Name' )",
				"vnetSiteID": "parameters( 'Virtual Network ID' )",
				"subnetId": "parameters( 'Virtual Network Subnet ID' )"
			},
			"dependsOn":
			[
			]
		}
	],
	"outputs":
	{
		"resourceId":
		{
			"type": "string",
			"value": "[ resourceId( 'Microsoft.Compute/virtualMachines', parameters( 'Virtual Machine Name' ) ) ]"
		},
		"resourceInfo":
		{
			"value": "[ reference( concat( 'Microsoft.Compute/virtualMachines/', parameters( 'Virtual Machine Name' ) ), providers( 'Microsoft.Compute', 'virtualMachines' ).apiVersions[ 0 ] ) ]",
			"type": "object"
		}
	}
}