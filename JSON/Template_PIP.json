{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters":
	{
		"PIP Name":
		{
			"type": "string"
		},
		"PIP Allocation":
		{
			"type": "string"
		}
	},
	"variables":
	{
	},
	"functions":
	[
	],
	"resources":
	[
		{
			"comments": "PIP",
			"type": "Microsoft.Network/publicIPAddresses",
			"apiVersion": "2018-08-01",
			"name": "[ parameters( 'PIP Name' ) ]",
			"location": "[ resourceGroup().location ]",
			"properties":
			{
				"publicIPAddressVersion": "IPv4",
				"publicIPAllocationMethod": "[ parameters( 'PIP Allocation' ) ]"
			},
			"sku":
			{
				"name": "Basic"
			},
			"dependsOn":
			[
			]
		}
	],
	"outputs":
	{
		"resourceId":
		{
			"type": "string",
			"value": "[ resourceId( 'Microsoft.Network/publicIPAddresses', parameters( 'PIP Name' ) ) ]"
		},
		"resourceInfo":
		{
			"type": "object",
			"value": "[ reference( concat( 'Microsoft.Network/publicIPAddresses/', parameters( 'PIP Name' ) ), providers( 'Microsoft.Network', 'publicIPAddresses' ).apiVersions[ 0 ] ) ]"
		}
	}
}